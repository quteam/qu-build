{
  "extends": "eslint-config-airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "mocha": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "rules": {
    "array-callback-return": 0,
    "indent": 0,
    "linebreak-style": 0,
    "no-console": 0,
    "no-underscore-dangle": 0
  }
}
