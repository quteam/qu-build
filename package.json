{
  "name": "qu-build",
  "version": "1.3.2",
  "description": "Build tools for FE",
  "bin": {
    "qu-build": "./bin/build",
    "qu-build-init": "./bin/build-init"
  },
  "main": "lib/build.js",
  "module": "es/build.js",
  "files": [
    "bin",
    "es",
    "lib",
    "template"
  ],
  "scripts": {
    "clean": "rimraf dist lib es",
    "build:commonjs": "better-npm-run build-commonjs",
    "build:es": "better-npm-run build-es",
    "build": "npm run clean && npm run build:commonjs && npm run build:es",
    "lint": "eslint --ext .js src",
    "lint:fix": "eslint --fix --ext .js src",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "build-commonjs": {
      "command": "babel src --out-dir lib",
      "env": {
        "BABEL_ENV": "commonjs"
      }
    },
    "build-es": {
      "command": "babel src --out-dir es",
      "env": {
        "BABEL_ENV": "es"
      }
    },
    "test": {
      "command": "jest --config jest.json"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:quteam/qu-build.git"
  },
  "keywords": [
    "webpack",
    "typescript",
    "es",
    "babel"
  ],
  "author": "haovei <haovei@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^7.2.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.3.0",
    "commander": "^2.12.2",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.7",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^22.0.3",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "imagemin-webpack-plugin": "^1.5.2",
    "jest": "^21.2.1",
    "json-loader": "^0.5.7",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "less-plugin-npm-import": "^2.1.0",
    "map-json-webpack-plugin": "^1.2.0",
    "mockjs": "^1.0.1-beta3",
    "ncp": "^2.0.0",
    "node-walkdir": "^0.1.1",
    "postcss": "^6.0.14",
    "postcss-loader": "^2.0.9",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "eslint": "^4.12.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "i18n-webpack-plugin": "^1.0.0"
  },
  "engines": {
    "node": ">= 8.5.0"
  }
}
