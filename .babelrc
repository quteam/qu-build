{
  "presets": [
    [
      "@babel/env",
      {
        "modules": false,
        "useBuiltIns": "usage",
        "loose": true
      }
    ]
  ],
  "env": {
    "commonjs": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "browsers": [
                "Android >= 4",
                "Chrome >= 35",
                "Firefox >= 31",
                "iOS >= 9",
                "Opera >= 12",
                "Safari >= 9",
                "IE >= 9"
              ]
            },
            "useBuiltIns": "usage"
          }
        ]
      ]
    },
    "es": {
      "presets": [
        [
          "@babel/env",
          {
            "modules": false,
            "useBuiltIns": "usage"
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "@babel/env"
        ],
        "@babel/react"
      ]
    }
  },
  "plugins": [
    ["@babel/plugin-proposal-decorators", {
      "legacy": true
    }],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  ],
  "comments": false
}
